<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- pressed -->
    <item android:drawable="@drawable/energy_pressed" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/energy_pressed" android:state_focused="true"/>
    <!-- checked -->
    <item android:drawable="@drawable/energy_pressed" android:state_checked="true"/>
    <!-- default -->
    <item android:drawable="@drawable/energy"/>
</selector>